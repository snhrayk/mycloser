datasource db {
  provider = "mysql"
}

generator client{
  provider = "prisma-client-js"
}

model webZemi_Users {
  id              Int     @id @default(autoincrement())
  class_name      String? @db.VarChar(10)
  graduation_year Int?
  name            String? @db.VarChar(50)
  username        String? @db.VarChar(50)
  password        String? @db.VarChar(255)

  @@map("webZemi_users")
}
